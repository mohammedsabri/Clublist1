<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ie.wit.clublist">
    <!-- application package name -->
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:name=".main.MainApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_clublist_round"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_clublist_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Clublist">


        <activity android:name=".activities.ClublistActivity" ></activity>

        <activity android:name=".activities.ClublistListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>


    </application>


</manifest>